{
  "name": "@openmrs/angularjs-openmrs-api",
  "version": "0.5.3",
  "description": "AngularJS (1.x) library for OpenMRS API",
  "main": "lib/index.js",
  "engines": {
    "node": "^6.10.0"
  },
  "scripts": {
    "clean": "rimraf lib",
    "build:dev": "webpack --progress --colors --env=dev",
    "build:prod": "webpack --progress --colors --env=prod",
    "build": "rimraf lib && karma start && webpack --progress --colors --env=prod",
    "test": "karma start",
    "test:dev": "karma start --singleRun=false",
    "prepublish": "rimraf lib && karma start && webpack --progress --colors --env=prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openmrs/openmrs-web-angularjs-api.git"
  },
  "keywords": [
    "angularjs",
    "openmrs"
  ],
  "author": "rkorytkowski",
  "license": "MPL-2.0",
  "homepage": "https://github.com/openmrs/openmrs-web-angularjs-api#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "angular-mocks": "~1.5.8",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "jasmine-core": "^2.6.2",
    "json-loader": "^0.5.4",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "rimraf": "^2.6.1",
    "webpack": "^2.5.1",
    "webpack-node-externals": "^1.6.0",
    "yargs": "^8.0.1"
  },
  "dependencies": {
    "angular": ">=1.5 <=1.6",
    "angular-resource": ">=1.5 <=1.6",
    "angular-translate": "~2.15.1",
    "angular-translate-loader-url": "^2.11.0"
  }
